<div class="row">
  <div class="medium-12 columns">
    <h1 class="text-center panel"> AJAX Puppy HQ  </h1>

    <table class="medium-6 medium-offset-3 columns">
      <th> Name </th>
      <th> Breed </th>
      
      <% @puppies.each do |puppy| %>
        <tr>
          <td> <%= puppy.name %> </td>
          <td> <%= puppy.breed.name %> </td>
        </tr>
      <% end %>
    </table>
  </div>
</div>